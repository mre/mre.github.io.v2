{% if is_post %}
<footer>
    {%- include _layouts/subscribe.liquid %}
    <h3>Continue reading</h3>
    <ul>
        {% for post in posts limit:5 %}
        {% if post.title != title %}
            <li><a href="{{post.path}}">{{post.title}}</a></li>
        {% endif %}
        {% endfor %}
    </ul>
</footer>
{% endif %}
